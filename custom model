import 'package:flutter/material.dart';

class NewModel {
  final String name;
  final String image;
  final String detail;
  final String price;
  bool isFavorite;



  NewModel({
    required this.name,
    required this.image,
    required this.detail,
    required this.price,
    this.isFavorite = false, // default to false
  });
}
